{
	"editor.suggest.showInlineDetails": false,
	"editor.inlineSuggest.enabled": false,
	"git.showInlineOpenFileAction": false,
	"editor.snippetSuggestions": "bottom",
	"gitHistory.sourceCodeProviderIntegrationLocation": "More Actions",
	"tailwindCSS.colorDecorators": false,
	"scss.lint.propertyIgnoredDueToDisplay": "ignore",
	"less.lint.propertyIgnoredDueToDisplay": "ignore",
	"audioCues.lineHasInlineSuggestion": "off",
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"workbench.colorTheme": "Atom One Dark",
	"path-intellisense.autoTriggerNextSuggestion": true,
	"typescript.suggestionActions.enabled": false,
	"editor.quickSuggestionsDelay": 100,
	"tailwindCSS.suggestions": false,
	"editor.suggest.preview": true,
	"rainbowTags.allowEverywhere": true,
	"diffEditor.ignoreTrimWhitespace": true,
	"editor.autoIndent": "advanced",
	"editor.codeLens": false,
	"html.format.indentInnerHtml": true,
	"prettier.useTabs": true,
	"html.format.endWithNewline": true,
	"html.format.preserveNewLines": false,
	"editor.wordWrap": "on"
}
